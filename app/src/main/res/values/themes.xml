<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="Theme.StorageManipulator" parent="Theme.Material3.Dark">
        <!-- Primary brand color. -->
        <item name="colorPrimary">@color/reddish_brown</item>
        <item name="colorPrimaryVariant">@color/reply_dark_blue</item>
        <item name="colorOnPrimary">@color/reply_orange</item>

        <item name="colorOnSurface">@color/reply_orange</item>
        <item name="android:textColorHint">@color/black</item>

        <!-- Secondary brand color. -->
        <item name="colorSecondary">@color/teal_200</item>
        <item name="colorSecondaryVariant">@color/teal_700</item>
        <item name="colorOnSecondary">@color/black</item>
        <!-- Status bar color. -->
        <item name="android:statusBarColor" tools:targetApi="l">@color/black</item>

        <!-- Customize your theme here. -->
        <item name="colorPrimaryContainer">@color/reply_light_yellow</item>
        <item name="colorOnPrimaryContainer">@color/reply_biege</item>
        <item name="colorOutline">@color/reply_light_yellow</item>



    </style>
</resources>